<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Lớp nền (background chính) -->
    <item android:width="94.67dp"
        android:height="94.67dp">
          android:top="4dp"
          android:bottom="4dp"
          android:left="4dp"
          android:right="4dp">
        <shape android:shape="rectangle">
            <solid android:color="#000000"/> <!-- Màu nền trắng -->
        </shape>
    </item>

    <!-- Lớp viền trong (inner border) -->
    <item android:top="2dp" android:bottom="0dp" android:left="0dp" android:right="2dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF"/> <!-- Màu viền đen -->
        </shape>
    </item>

</layer-list>


